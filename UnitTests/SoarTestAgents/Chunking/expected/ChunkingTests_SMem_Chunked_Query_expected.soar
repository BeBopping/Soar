sp {answer*retrieve-goal*copy-goal*StateNoChange*t5-1
    (state <s1> ^cue2 goal -^deleted <d*1> -^goal <g*1> ^cue1 handle
          ^superstate nil)
    -->
    (<s1> ^goal <l4> +)
    (<l4> ^predicate <l3> + ^procedural <l5> + ^handle goal +)
    (<l3> ^1 <l2> +)
    (<l2> ^default <l1> +)
    (<l1> ^name pantry +)
    (<l5> ^arg1 <l6> +)
    (link-stm-to-ltm <l1> 4)
    (link-stm-to-ltm <l2> 3)
    (link-stm-to-ltm <l3> 5)
    (link-stm-to-ltm <l4> 1)
    (link-stm-to-ltm <l5> 2)
    (link-stm-to-ltm <l6> 6)
}